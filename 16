package stringtest;

public class StringTest2 {
    public static void main(String[] args) {
        String s1 = "hello";
        String s2 = "hel";
        String s3 = "lo";
        String s4 = s2 + s3;
        System.out.println(s1 == s4);
        System.out.println("===================");

        String s5 = new String("hello");
        String s6 = s1.intern();
        System.out.println(s1 == s6);
        System.out.println(s5 == s6);
        System.out.println(s4 .equals( s5));
    }
}
